{
    "gitbook": ">=3.2.2",
    "plugins": [
        "anchors",
        "include-codeblock",
        "ga",
        "richquotes",
        "toc",
        "expandable-chapters",
        "-search",
        "mathjax",
        "-code-highlighter"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "ga": {
            "token": "UA-65761593-1"
        },
        "richquotes": {
            "todos": true,
            "fixme": true
        }
    },
    "links": {
        "sidebar": {
            "Feel++": "http://www.feelpp.org/",
            "GitHub": "http://www.github.com/feelpp/feelpp"
        }
    },
    "variables": {
        "feelpp": "Feel++"
    }
}
